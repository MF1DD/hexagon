{
  "timeout": 10,
  "source": {
    "directories": [
      "/app/.qa",
      "/app/src"
    ],
    "excludes": [
      "/app/src/App/UserInterface/CLI/PHPUnit",
      "/app/.qa/phpstan"
    ]
  },
  "phpUnit": {
    "customPath": "/app/vendor/bin/phpunit",
    "configDir": "/app/.qa/phpunit"
  },
  "logs": {
    "text": "/app/.qa/infection/logs/infection.log",
    "summary": "/app/.qa/infection/logs/infection-summary.log",
    "perMutator": "/app/.qa/infection/logs/per-mutator.log"
  },
  "mutators": {
    "@default": true,
    "PublicVisibility": false
  }
}
